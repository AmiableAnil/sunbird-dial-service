{
  "$id": "file:./schemas/dialcode/schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "dialcode",
  "type": "object",
  "required": [
    "identifier",
    "channel",
    "status",
    "generatedOn"
  ],
  "properties": {
    "identifier": {
      "type": "string"
    },
    "channel": {
      "type": "string"
    },
    "publisher" : {
      "type": "string"
    },
    "batchCode" : {
      "type": "string"
    },
    "status" : {
      "type": "string",
      "enum": [
              "Draft",
              "Review",
              "Live"
      ],
      "default": "Draft"
    },
    "generatedOn": {
      "type": "string"
    },
    "publishedOn": {
      "type": "string"
    }
  }
}
